    "babel-preset-stage-2": "6.24.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "jest-extended": "0.11.5",
    "rimraf": "3.0.2",
    "webpack": "5.24.3",
